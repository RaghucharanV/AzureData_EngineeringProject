{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "olympicdf8"
		},
		"raw_data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'raw_data'"
		},
		"Athletes_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Athletes.csv"
		},
		"raw_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tokyodata1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "olympicdf8_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"raw_data_accountKey": {
						"value": "[parameters('raw_data_accountKey')]"
					},
					"Athletes_properties_typeProperties_url": {
						"value": "[parameters('Athletes_properties_typeProperties_url')]"
					},
					"raw_data_properties_typeProperties_url": {
						"value": "[parameters('raw_data_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}